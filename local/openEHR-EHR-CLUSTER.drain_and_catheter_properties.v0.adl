definition
	CLUSTER[at0000] matches {    -- drain_and_catheter_properties
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Activation
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0003,    -- Active
							at0004]    -- Passive
						}
					}
				}
			}
			ELEMENT[at0005] occurrences matches {0..1} matches {    -- State
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0006,    -- Open
							at0007]    -- Closed
						}
					}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Suction level
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::125]>
						list = <
							["1"] = <
								units = <"mm[Hg]">
							>
						>
					>
				}
			}
		}
	}

